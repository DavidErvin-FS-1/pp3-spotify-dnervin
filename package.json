{
  "name": "pp3-spotify-dnervin",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "install:client": "cd client && npm install",
    "install:server": "cd server && npm install",
    "install:all": "concurrently \"npm run install:client\" \"npm run install:server\"",
    "build:client": "cd client && npm run build",
    "start": "concurrently \"npm run build:client\" \"cd server && npm run dev\""
  },
  "author": "David Ervin",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
